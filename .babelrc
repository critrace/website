{
  "presets": ["@babel/preset-typescript", ["@babel/preset-env", {
    "targets": {
      "browsers": ["last 2 Chrome versions"]
    }
  }], "@babel/preset-react"],
  "plugins": ["transform-class-properties",
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }]
  ]
}
