{
  "name": "@critresult/website",
  "version": "0.0.1",
  "description": "The critresult.com user interface",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r ./build || true",
    "build": "npm run clean && webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --config webpack.config.js",
    "lint": "eslint ./src/**.tsx",
    "now-build": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "extends": [
      "@jchancehud/eslint-config-typescript"
    ],
    "rules": {
      "semi": [
        1,
        "never"
      ],
      "prettier/prettier": [
        1,
        {
          "trailingComma": "es5",
          "arrowParens": "always",
          "singleQuote": true,
          "semi": false
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/critresult/website.git"
  },
  "author": "Chance Hudson",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/critresult/website/issues"
  },
  "homepage": "https://github.com/critresult/website#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "email-validator": "^2.0.4",
    "jsonwebtoken": "^8.5.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@jchancehud/eslint-config-typescript": "0.0.2",
    "@types/axios": "^0.14.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.3",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
